@model MvcOnlineTicariOtomasyon.Models.Classes.Department

@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<h2>Departman Ekle</h2>

@using (Html.BeginForm("Create", "Department", FormMethod.Post))
{
    <div class="mb-3">
        @Html.Label("Departman Adı")
        @Html.TextBoxFor(x => x.DepartmentName, new { @class = "form-control" })
    </div>
    <button class="btn btn-info">Ekle</button>
}


